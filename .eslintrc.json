{
    "extends": "airbnb-base",
    "rules": {
      "arrow-parens": 0,
      "implicit-arrow-linebreak": 0,
      "no-console": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "^err$|^req$|^res$|^next$" }]
    },
    "env": {
      "jest": true
    }
}
